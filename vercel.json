{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "chmod a+x build.sh && ./build.sh",
  "outputDirectory": "public",
  "functions": {
    "api/*.py": {
      "runtime": "python@3.11"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/main.py"
    }
  ]
}